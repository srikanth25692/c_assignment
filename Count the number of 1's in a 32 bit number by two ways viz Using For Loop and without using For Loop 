#include <stdio.h>
int main()
{
   int a=11,count=0;
   /******************************/
   //with for loop
   //for(int i=0;i<32;i++)
   //{
     //  if(a&(1<<i))
     //  {
     //  count++;
     //  }
   //}
   /******************************/
   /******************************/
   //without for loop
   int i=0;
   while(i<32)
   {
       if(a&(1<<i))
       {
       count++;
       }
       i++;
   }
   /******************************/
   printf("%d\t",count);
    return 0;
}
