#include <stdio.h>

int main()
{
    int number=0;
    for(int i=1;i<100;i++)
    {
     int prime=0;
     number=i;
     if(number==1)
     {
         printf("Number is not prime\n");
         prime=1;   
     }
     else if(number==2)
     {
         printf("Number is prime\n");
         prime=1;   
     }
     else if(number>2)
     {
         for(int j=2;j<=(number/2);j++)    
     {    
     if(number%j==0)    
     {    
     printf("Number is not prime\n");    
     prime=1;    
     break;
     }    
     }
     }
     if(prime==0)
     {
     printf("Number is prime\n");
     }
     }
    return 0;
}
