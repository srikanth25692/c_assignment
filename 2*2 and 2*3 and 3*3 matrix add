#include <stdio.h>

int main()
{
    int a[2][2]={0,1,2,3},b[2][3]={0,1,2,3,4,5},c[3][3]={0,1,2,3,4,5,6,7,8},d[2][3]={0},e[2][3]={0};
    for(int i=0;i<2;i++)
    {
      for(int j=0;j<3;j++)
    {
          for(int k=0;k<2;k++)
    {
        d[i][j] += a[i][k] + b[k][j];
    }  
    }  
    }
    for(int i=0;i<2;i++)
    {
      for(int j=0;j<3;j++)
    {
          for(int k=0;k<3;k++)
    {
        e[i][j] += d[i][k] + c[k][j];
    }  
    }  
    }
    for(int i=0;i<2;i++)
    {
      for(int j=0;j<3;j++)
    {
    printf("%d\t",e[i][j]);
    }  
    }
    return 0;
}
